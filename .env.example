FIREBASE_SERVICE_ACCOUNT_KEY_PATH=./service-account.json
# Additional SNS keys from original .env
X_API_KEY=
X_API_SECRET_KEY=
X_ACCESS_TOKEN=
X_ACCESS_TOKEN_SECRET=
# X API Bearer Token (선택 - analytics/trendScraper의 읽기 전용 API 호출에 사용. 미설정 시 OAuth 1.0a로 폴백)
X_BEARER_TOKEN=
INSTAGRAM_BUSINESS_ACCOUNT_ID=
INSTAGRAM_ACCESS_TOKEN=

# Telegram Admin Bot
TELEGRAM_BOT_TOKEN=
TELEGRAM_ADMIN_CHAT_ID=

# fal.ai Image Generation
FAL_AI_KEY=
FAL_MODEL=fal-ai/flux-pro/v1.1-ultra
# 패션 에디토리얼: FLUX 1.1 Pro Ultra (최고 품질, ~$0.06/이미지)
# 카드뉴스 커버: Recraft V3 (타이포그래피 특화, $0.04/이미지) - 자동 사용

# Gemini AI (brainstorm)
GEMINI_API_KEY=

# Anthropic AI (Hybrid Polishing)
ANTHROPIC_API_KEY=

# Naver News API (선택 - 한국어 뉴스 수집 + DataLab 트렌드. 미설정 시 RSS/Reddit만 사용)
# https://developers.naver.com/apps/ 에서 무료 발급 (일 25,000 호출)
NAVER_CLIENT_ID=
NAVER_CLIENT_SECRET=

# YouTube Data API (선택 - K-POP MV 조회수 트렌드 화제성 분석. 미설정 시 Google Trends/Naver만 사용)
# https://console.cloud.google.com/apis/credentials 에서 무료 발급 (일 10,000 유닛)
YOUTUBE_API_KEY=

# Firebase Storage
FIREBASE_STORAGE_BUCKET=my-style-5649d.appspot.com
